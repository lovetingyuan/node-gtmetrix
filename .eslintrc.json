{
  "extends": "airbnb-base",
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "no-console": [
      "off"
    ],
    "linebreak-style": [
      "error",
      "windows"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "no-useless-escape": [
      "off"
    ]
  }
}
